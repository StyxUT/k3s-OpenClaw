---
apiVersion: v1
kind: Secret
metadata:
  name: openclaw-gog
  namespace: openclaw
  labels:
    app: openclaw-gateway
type: Opaque
stringData:
  # gogcli Desktop OAuth client JSON (downloaded from Google Cloud Console).
  # This should be the full JSON content.
  # Do NOT commit real credentials.
  credentials.json: |-
    {
      "installed": {
        "client_id": "REPLACE_ME.apps.googleusercontent.com",
        "project_id": "REPLACE_ME",
        "auth_uri": "https://accounts.google.com/o/oauth2/auth",
        "token_uri": "https://oauth2.googleapis.com/token",
        "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
        "client_secret": "REPLACE_ME",
        "redirect_uris": [
          "http://localhost"
        ]
      }
    }

  # Default Gmail account to use with gog.
  # Do NOT commit real email addresses if this repo is public.
  account: "REPLACE_ME"

  # Password used by gogcli when running in a container (file keyring backend).
  # Choose a long random value.
  # Do NOT commit a real password.
  keyring_password: "REPLACE_ME"
